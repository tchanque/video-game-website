40%) {\n        @return #000;\n    } @else {\n        @return #fff;\n    }\n}\n","scss/_pagelist.scss":".welcome-container {\n    \n    * {\n        margin: 1rem 0;\n    }\n}\n\n.articles {\n    display: grid;\n    grid-template-columns: repeat(3, 1fr);\n    grid-gap: 20px;\n\n    .cardGame {\n        &__imgWrapper{\n            position: relative;\n\n            &__img {\n                display: block;\n                width: 100%;\n                height: 20vw;\n                object-fit: cover;\n                transition: opacity 0.3s ease; /* Add transition effect for opacity */\n            }\n\n            &__text {\n                position: absolute;\n                top: 50%;\n                left: 50%;\n                transform: translate(-50%, -50%);\n                transition: opacity 0.3s ease; /* Add transition effect for opacity */\n                opacity: 0;\n            }\n\n            &:hover .cardGame__imgWrapper__img {\n                opacity: 0.7; /* Decrease opacity of image on hover */\n            }\n              \n            &:hover .cardGame__imgWrapper__text {\n                opacity: 1; /* Show text on hover */\n            }\n        }\n    \n        &__platforms {\n            &__logo {\n                padding-right: 0.5rem;\n            }\n        }\n    }    \n\n    .display_off {\n        display: none;\n    }\n}\n\n\n.show-more {\n    display: flex;\n    justify-content: center;\n    align-items: center;\n    padding: 4rem 0;\n\n    &__btn {\n        border-radius: 0px;\n        border: none;\n        padding: 1rem 4rem;\n        background: $secondary-color;\n        color: #fff;\n        cursor: pointer;\n    }\n}","scss/_pagedetail.scss":".game-hero {\n    position: relative;\n    margin: 2rem 0;\n\n    &__backgroundImg {\n        width: 100%;\n        height: 50vw;\n        object-fit: cover;\n    }\n\n    &__websiteBtn {\n        position: absolute;\n        bottom: 2rem;\n        right: 2rem;\n        padding: 0.75rem;\n        background: $secondary-color;\n        border: none;\n        color: #fff;\n        text-decoration: none;\n        font-size: 400;\n        font-weight: bold;\n\n        display: flex;\n        align-items: center;\n        justify-content: space-between;\n    }\n\n    &__websiteBtn p {\n        margin-right: 2rem\n    }\n}\n\n.game-detail {\n\n    *{\n        padding: 0.5rem 0;\n    }\n\n    &__header {\n        display: flex;\n        justify-content: space-between;\n        align-items: end;\n\n        &__ratings {\n            color: $secondary-color;\n        }\n    }\n\n    &__release {\n        display: flex;\n        justify-content: start;\n\n        * {\n            flex: 1 ;\n            margin-right: 1rem;\n        }\n    }\n\n    &__classification {\n        display: flex;\n        justify-content: start;\n\n        * {\n            flex: 1 ;\n            margin-right: 1rem;\n        }\n    }\n}\n\n.game-buy {\n    h1{\n        color: $secondary-color;\n    }\n\n    &__stores {\n        padding: 2rem 0;\n\n        &__store {\n            display: flex;\n            padding: 0.5rem 0;\n\n            a {\n                color: #fff;\n                text-decoration: none;\n                border-bottom: 0.25rem solid $secondary-color;\n                margin-right: .5rem;\n            }\n        }\n    }\n}\n\n.game-trailer {\n    h1 {\n       color: $secondary-color; \n    }\n\n    &__video {\n        width: 100%;\n        margin: 2rem 0;\n    }\n}\n\n.game-screenshots {\n    padding: 2rem 0;\n\n    h1 {\n        color: $secondary-color;\n    }\n\n    &__pictures {\n        display: grid;\n        grid-template-columns: repeat(2, 2fr);\n        grid-gap: 40px;\n        padding-top: 2rem;\n\n        img {\n            width: 100%;\n            height: 25vw;\n            object-fit: cover;\n        }\n    }\n}\n\n.youtube {\n    h1 {\n        color: $secondary-color;\n    }\n}\n\n.similar-games {\n    h1 {\n        color: $secondary-color;\n    }\n}\n","scss/main.scss":"@import 'config';\n@import 'pagelist';\n@import 'pagedetail';\n\nbody {\n    background: $primary-color;\n    color: set-text-color($primary-color);\n    display: flex;\n    justify-content: center;\n    flex-direction: column;\n    padding: 2rem 3rem;\n    font-family: Arial, Helvetica, sans-serif;\n}\n\nh1 {\n    font-weight: 600;\n    font-size: 2.125rem;\n}\n\nh2 {\n    font-size: 1.75rem;\n}\n\nheader {\n    display: flex;\n    justify-content: space-between;\n    align-items: center;\n    z-index: 2;\n    width: 100%;\n    padding-bottom: 2rem;\n\n    .logo {\n        font-weight: 600;\n        font-size: 2.125rem;\n        color: $secondary-color;\n        text-decoration: none;\n    }\n\n    .search-bar {\n        display: flex;\n        justify-content: center;\n        align-items: center;\n        border: 1px solid ;\n        border-radius: 15px;\n        padding: 0.25rem 0.7rem;\n    }\n\n    .search-input {\n        background: transparent;\n        border-color: transparent;\n        padding: 0 0.25rem;\n        color: #fff;\n        outline: none; // remove the outline effect (highlight the contour of the input area) when focusing on the text area (click)\n    }\n\n    .svg {\n        margin-right: 0.25rem;\n    }\n    \n}\n\n.footer {\n    position: fixed;\n    left: 0;\n    bottom: 0;\n    width: 100%;\n}\n\n"},"lineCount":null}},"error":null,"hash":"84c843097c4a4d193ceece4d4343073b","cacheData":{"env":{}}}